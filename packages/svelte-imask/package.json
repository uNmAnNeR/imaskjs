{
  "name": "@imask/svelte",
  "author": "Alexey Kryazhev",
  "license": "MIT",
  "version": "7.6.1",
  "description": "Svelte input mask",
  "main": "dist/svelte-imask.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "repository": "https://github.com/uNmAnNeR/imaskjs/tree/master/packages/svelte-imask",
  "scripts": {
    "lint": "eslint src",
    "prebuild": "npm run lint -- --quiet && rimraf --glob \"{dist,esm}\"",
    "build": "tsc --emitDeclarationOnly && rollup -c"
  },
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./dist/svelte-imask.js",
      "default": "./dist/svelte-imask.js"
    },
    "./esm": {
      "types": "./esm/index.d.ts",
      "default": "./esm/index.js"
    },
    "./esm/*": {
      "types": "./esm/*.d.ts",
      "default": "./esm/*.js"
    },
    "./esm/*.js": {
      "types": "./esm/*.d.ts",
      "default": "./esm/*.js"
    },
    "./*": {
      "types": "./esm/*.d.ts",
      "default": "./esm/*.js"
    },
    "./*.js": {
      "types": "./esm/*.d.ts",
      "default": "./esm/*.js"
    }
  },
  "typesVersions": {
    ">=4.2": {
      "*": [
        "esm/*"
      ],
      "esm/*": [
        "esm/*"
      ]
    }
  },
  "keywords": [
    "svelte",
    "imask",
    "javascript",
    "input",
    "mask"
  ],
  "dependencies": {
    "imask": "^7.6.1"
  },
  "peerDependencies": {
    "svelte": "^4.2.17"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "228f2a4dce7125d0c7156868fcb5364b4487c8e6"
}
