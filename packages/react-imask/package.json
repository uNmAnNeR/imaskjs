{
  "name": "react-imask",
  "version": "7.6.1",
  "license": "MIT",
  "author": "Alexey Kryazhev",
  "homepage": "https://imask.js.org/",
  "description": "React input mask",
  "main": "dist/react-imask.js",
  "module": "esm/index.js",
  "type": "module",
  "types": "index.d.ts",
  "repository": "https://github.com/uNmAnNeR/imaskjs/tree/master/packages/react-imask",
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./dist/react-imask.cjs",
      "default": "./dist/react-imask.js"
    },
    "./esm": {
      "types": "./esm/index.d.ts",
      "default": "./esm/index.js"
    },
    "./esm/*": {
      "types": "./esm/*.d.ts",
      "default": "./esm/*.js"
    },
    "./esm/*.js": {
      "types": "./esm/*.d.ts",
      "default": "./esm/*.js"
    },
    "./*": {
      "types": "./esm/*.d.ts",
      "default": "./esm/*.js"
    },
    "./*.js": {
      "types": "./esm/*.d.ts",
      "default": "./esm/*.js"
    }
  },
  "typesVersions": {
    ">=4.2": {
      "*": [
        "esm/*"
      ],
      "esm/*": [
        "esm/*"
      ]
    }
  },
  "scripts": {
    "lint": "eslint src",
    "prebuild": "npm run lint -- --quiet && rimraf --glob \"{dist,esm}\"",
    "build": "tsc --emitDeclarationOnly && rollup -c",
    "watch": "rollup -c -w"
  },
  "keywords": [
    "react",
    "imask",
    "input",
    "mask"
  ],
  "dependencies": {
    "imask": "^7.6.1",
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "react": ">=0.14.0"
  },
  "engines": {
    "npm": ">=4.0.0"
  },
  "gitHead": "228f2a4dce7125d0c7156868fcb5364b4487c8e6",
  "devDependencies": {
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
