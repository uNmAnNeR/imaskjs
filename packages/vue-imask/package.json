{
  "name": "vue-imask",
  "author": "Alexey Kryazhev",
  "license": "MIT",
  "version": "7.1.0",
  "description": "Vue input mask",
  "main": "dist/vue-imask.js",
  "module": "esm/index.js",
  "type": "module",
  "types": "esm/index.d.ts",
  "repository": "https://github.com/uNmAnNeR/imaskjs/tree/master/packages/vue-imask",
  "contributors": [
    "naprimer",
    "Yacine Hmito <yacine.hmito@gmail.com>",
    "cangku"
  ],
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "default": "./dist/vue-imask.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "lint": "eslint src",
    "prebuild": "npm run lint -- --quiet && rimraf --glob \"{dist,esm}\"",
    "build": "tsc --emitDeclarationOnly && rollup -c",
    "watch": "rollup -c -w"
  },
  "keywords": [
    "vue",
    "imask",
    "javascript",
    "input",
    "mask"
  ],
  "dependencies": {
    "imask": "^7.1.0-alpha.2",
    "vue-demi": "^0.14.5"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1.0.0",
    "vue": ">=2.2.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "gitHead": "228f2a4dce7125d0c7156868fcb5364b4487c8e6",
  "devDependencies": {
    "vue": "^2.7.14"
  }
