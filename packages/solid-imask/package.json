{
  "name": "solid-imask",
  "version": "7.6.1",
  "license": "MIT",
  "author": "Alexey Kryazhev",
  "description": "Solid input mask",
  "main": "dist/solid-imask.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "repository": "https://github.com/uNmAnNeR/imaskjs/tree/master/packages/solid-imask",
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./dist/solid-imask.js",
      "default": "./dist/solid-imask.js"
    },
    "./esm": {
      "types": "./esm/index.d.ts",
      "default": "./esm/index.js"
    },
    "./esm/*": {
      "types": "./esm/*.d.ts",
      "default": "./esm/*.js"
    },
    "./esm/*.js": {
      "types": "./esm/*.d.ts",
      "default": "./esm/*.js"
    },
    "./*": {
      "types": "./esm/*.d.ts",
      "default": "./esm/*.js"
    },
    "./*.js": {
      "types": "./esm/*.d.ts",
      "default": "./esm/*.js"
    }
  },
  "typesVersions": {
    ">=4.2": {
      "*": [
        "esm/*"
      ],
      "esm/*": [
        "esm/*"
      ]
    }
  },
  "scripts": {
    "lint": "eslint src",
    "prebuild": "npm run lint -- --quiet && rimraf --glob \"{dist,esm}\"",
    "build": "tsc --emitDeclarationOnly && rollup -c"
  },
  "keywords": [
    "solid",
    "imask",
    "input",
    "mask"
  ],
  "dependencies": {
    "imask": "^7.6.1"
  },
  "peerDependencies": {
    "solid-js": ">=1.3.0"
  },
  "devDependencies": {
    "babel-preset-solid": "^1.8.17",
    "solid-js": "^1.8.17"
  },
  "engines": {
    "npm": ">=4.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
